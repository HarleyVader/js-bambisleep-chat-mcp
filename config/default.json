{
  "embedding": {
    "providers": [
      {
        "name": "lmstudio",
        "url": "http://192.168.0.178:7777",
        "endpoint": "/v1/embeddings",
        "model": "text-embedding-nomic-embed-text-v1.5@q8_0",
        "dimensions": 1536,
        "priority": 1
      },
      {
        "name": "openai",
        "url": "https://api.openai.com",
        "endpoint": "/v1/embeddings",
        "model": "text-embedding-3-small",
        "dimensions": 1536,
        "priority": 2
